clear;
close all;

load('mst.txt');
load('slv.txt');


mst_refX = mst(:,1);
mst_outX = mst(:,2);

slv_refX = slv(:,1);
slv_outX = slv(:,2);


mst_refY = mst(:,1);
mst_outY = mst(:,2);

slv_refY = slv(:,1);
slv_outY = slv(:,2);


mst_refZ = mst(:,1);
mst_outZ = mst(:,2);

slv_refZ = slv(:,1);
slv_outZ = slv(:,2);

figure(1);
subplot(2,3,1);
plot(mst_refX,'r');
hold on;
plot(-mst_outX,'b');
grid;
legend('Refef','Out');
title('Master');

subplot(2,3,2);
plot(slv_refX,'r');
hold on;
plot(-slv_outX,'b');
grid;
legend('Ref','Out');
title('Slave');


subplot(2,3,3);
plot(mst_refY,'r');
hold on;
plot(-mst_outY,'b');
subplot(2,3,1);
plot(mst_refY,'r');
hold on;
plot(-mst_outY,'b');
grid;
legend('Refef','Out');
title('Master');

subplot(2,3,4);
plot(slv_refZ,'r');
hold on;
plot(-slv_outZ,'b');
grid;
legend('Ref','Out');
title('Slave');
grid;
legend('Refef','Out');
title('Master');

subplot(2,3,5);
plot(slv_refZ,'r');
hold on;
plot(-slv_outZ,'b');
grid;
legend('Ref','Out');
title('Slave');

